install.packages('tidyr')
library(tidyr)
str_squish(data1)
str_replace_all(data1,",","")
strsplit(data1,split=" ")
unlist(data1)
seperate(data1)
x <- paste(data1, collapse = "")
unlist(strsplit(x, split = ""))
y = unlist(strsplit(x, split = " "))
gsub(" ", "", y, fixed = TRUE)
stri_remove_empty(y, na_empty = FALSE)
k = stri_remove_empty(y, na_empty = FALSE)
str_locate(k, '17a')
t = unlist(gregexpr("17a", k))
which(t == "True")
datanumber = which(t > 0)
